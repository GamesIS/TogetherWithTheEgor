@model IEnumerable<string>

<li>
    @Html.ActionLink("Все товары", "List", "Product", null,//Вот это удалить или добавить свои классы
    null)
</li>

@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link, new
{
    controller = "Product",
    action = "List",
    category = link,
    page = 1
}, null)
    </li>
}